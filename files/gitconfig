[alias]
  co = checkout
  st = status
  ci = commit
  commush = !git commit -a && git push
  cu = !git commit -a && git push
  df = !git diff --no-prefix && git diff --staged --no-prefix
  clear = reset --hard
  logp = log --pretty=oneline --abbrev-commit --decorate --graph
  logs = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --decorate --abbrev-commit
  stache = !echo "Waxing and twirling..." && echo && git stash $@
[color]
  ui = auto
[core]
  excludesfile = /home/webadmin/.global_gitignore
[push]
  default = tracking
[diff]
  renames = copy
[url "http://git.drupal.org/project/"]
  insteadOf = "do:"
[branch]
  autosetuprebase = always
